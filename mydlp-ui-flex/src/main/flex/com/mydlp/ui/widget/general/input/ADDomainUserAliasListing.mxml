<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:my="http://www.mydlp.com/flex/my"
		  xmlns:mx="library://ns.adobe.com/flex/mx" 
		  xmlns:domain="com.mydlp.ui.domain.*" 
		  xmlns:input="com.mydlp.ui.widget.general.input.*" 
		  width="{InputLabel.VERTICAL_INPUT_WIDTH}" height="150">
	<fx:Script>
		<![CDATA[
			import com.mydlp.ui.widget.general.popup.InputLabel;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<mx:ListCollectionView id="adDomainUserAliases"/>
	</fx:Declarations>

	<s:layout>
		<s:HorizontalLayout  verticalAlign="middle"/>
	</s:layout>
	
	<s:Group width="100%" height="100%">
		<s:layout>
			<s:HorizontalLayout verticalAlign="middle" gap="2"/>
		</s:layout>
		<s:List id="daList" dataProvider="{adDomainUserAliases}"  width="100%" height="100%" >
			<s:itemRenderer>
				<fx:Component>
					<s:ItemRenderer
						height="20"
						creationComplete="{data = data as ADDomainUserAlias}">
						<fx:Script>
							<![CDATA[
								import com.mydlp.ui.domain.ADDomainUserAlias;
							]]>
						</fx:Script>
						<s:layout>
							<s:HorizontalLayout paddingLeft="4" verticalAlign="middle" />
						</s:layout>
						<s:Label text="{data.userAlias}" />
					</s:ItemRenderer>
				</fx:Component>
			</s:itemRenderer>
		</s:List>
	</s:Group>
	
</s:SkinnableContainer>
